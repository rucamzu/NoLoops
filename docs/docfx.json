{
    "metadata": [
      {
        "src": [
          {
            "files": [
              "src/NoLoops/bin/Release/**/NoLoops.dll"
            ],
            "src": "../"
          }
        ],
        "dest": "api",
        "includePrivateMembers": false,
        "disableGitFeatures": false,
        "disableDefaultFilter": false,
        "noRestore": false,
        "namespaceLayout": "flattened",
        "memberLayout": "samePage",
        "allowCompilationErrors": false
      }
    ],
    "build": {
      "content": [
        {
          "files": [
            "toc.yml",
            "*.md"
          ]
        },
        {
          "files": [
            "api/index.md",
            "api/**.yml"
          ]
        },
        {
          "files": [
            "README.md"
          ],
          "src": "../",
          "dest": ""
        }
      ],
      "resource": [],
      "output": "_site",
      "globalMetadata": {
        "_appTitle": "NoLoops documentation",
        "_appName": "NoLoops",
        "_appFooter": "<span>NoLoops is licensed under the <a href=\"https://github.com/rucamzu/NoLoops/blob/main/LICENSE\" target=”_blank”>MIT</a> license. Copyright © 2025 <a href=\"mailto:ruben.campos.zurriaga@gmail.com\" target=”_blank”>Rubén Campos Zurriaga</a>. Made with <a href=\"https://dotnet.github.io/docfx\" target=”_blank”>docfx</a>.</span>",
        "_disableContribution": true
      },
      "globalMetadataFiles": [],
      "fileMetadataFiles": [],
      "template": [
        "default",
        "modern"
      ],
      "postProcessors": [],
      "keepFileLink": false,
      "disableGitFeatures": false
    }
  }